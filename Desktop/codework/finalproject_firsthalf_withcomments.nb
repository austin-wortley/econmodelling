Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{
    RowBox[{"a1", "-", "b1"}], "\[Equal]", "0"}], ",", 
   RowBox[{
    RowBox[{"a1", "-", 
     RowBox[{"b1", " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "1.5`"}], "/", "rho"}]]}]}], "\[Equal]", "100"}], ",", 
   RowBox[{
    RowBox[{"a2", "+", 
     RowBox[{"b2", " ", 
      RowBox[{"Log", "[", "rho", "]"}]}]}], "\[Equal]", "0"}], ",", 
   RowBox[{
    RowBox[{"a2", "+", 
     RowBox[{"b2", " ", 
      RowBox[{"Log", "[", 
       RowBox[{"1.5`", "\[VeryThinSpace]", "+", "rho"}], "]"}]}]}], 
    "\[Equal]", "100"}]}], "}"}]], "Output",
