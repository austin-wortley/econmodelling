Cell[BoxData[
 RowBox[{"eqns", "=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"expU", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
    RowBox[{
     RowBox[{"expU", "[", "1.5", "]"}], "\[Equal]", "100"}], ",", 
    RowBox[{
     RowBox[{"logU", "[", "0", "]"}], "\[Equal]", "0"}], ",", 
    RowBox[{
     RowBox[{"logU", "[", "1.5", "]"}], "\[Equal]", "100"}]}], 
   "}"}]}]], "Input",
